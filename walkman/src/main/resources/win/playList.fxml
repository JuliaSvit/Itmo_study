<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.Cursor?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.layout.VBox?>

<VBox xmlns="http://javafx.com/javafx/8.0.121" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.PlayList">
   <children>
      <Pane fx:id="plPanel" onMouseDragged="#setOnMouseDragged" onMouseExited="#setOnMousePressed" prefHeight="450.0" prefWidth="310.0">
         <children>
            <ListView fx:id="listView" layoutX="10.0" layoutY="20.0" prefHeight="430.0" prefWidth="300.0">
               <opaqueInsets>
                  <Insets bottom="5.0" left="10.0" right="10.0" top="10.0" />
               </opaqueInsets>
               <cursor>
                  <Cursor fx:constant="HAND" />
               </cursor>
               <padding>
                  <Insets bottom="5.0" left="10.0" right="10.0" top="10.0" />
               </padding>
            </ListView>
         </children>
      </Pane>
   </children>
</VBox>
